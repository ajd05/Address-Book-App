<center>
<h1 class="title">Contacts List</h1>

<div class="contactContainer">
<h2>Contacts</h2>
<% current_user.contacts.order("name ASC").each do |contact| %>
  <p>
    <strong>Name:</strong>
    <%= contact.name %>
    ||
    <strong>Email:</strong>
    <%= contact.email %>
    ||
      <tr>
      <td><%= link_to 'Show', contact_path(contact) %></td>
      ||
      <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
      ||
      <td><%= link_to 'Delete', [contact.user, contact],
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
    </p>
<% end %>
</center>
